<div>
    {{#each allProducts}}
    <p>
        id: {{this._id}}
        title: {{this.title}}
        description: {{this.description}}
        price: {{this.price}}
        thumbnail: {{this.thumbnail}}
        code: {{this.code}}
        stock: {{this.stock}}
        category: {{this.category}}
        status: {{this.status}}
    </p>
    <hr>
    {{/each}}
</div>